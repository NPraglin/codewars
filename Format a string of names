function list(names){
  // take an array and return a string with the values, seperated by commas and an & between the last two

  let returnedStr = ''
  for (let i = 0; i < names.length; i++) {
  if ( i == names.length - 1) {returnedStr += (names[i].name)} else if (i == names.length - 2) {returnedStr += (names[i].name + ' & ')}
  else {returnedStr += (names[i].name + ', ')}
    
  }
  
  return returnedStr
}
